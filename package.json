{
  "name": "ethglobal-january-2025",
  "version": "1.0.0",
  "description": "CDP AgentKit with 0x Price Oracle",
  "main": "dist/index.js",
  "bin": {
    "cdp-agent": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "start:api": "node dist/index.js serve",
    "start:chat": "node dist/index.js chat",
    "dev:api": "ts-node src/index.ts serve",
    "dev:chat": "ts-node src/index.ts chat",
    "debug:api": "ts-node --inspect src/index.ts serve",
    "debug:chat": "ts-node --inspect src/index.ts chat",
    "start": "echo 'Please use npm run start:api or npm run start:chat'"
  },
  "dependencies": {
    "@coinbase/agentkit": "^0.1.1",
    "@coinbase/agentkit-langchain": "^0.1.0",
    "@langchain/core": "^0.3.38",
    "@langchain/groq": "^0.1.3",
    "@langchain/langgraph": "^0.2.44",
    "@langchain/openai": "^0.4.2",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "reflect-metadata": "^0.2.1",
    "viem": "^2.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}